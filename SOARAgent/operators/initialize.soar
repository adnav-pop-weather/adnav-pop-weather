echo "\nLoading initialize.soar"

sp {apply*initialize
    (state <s> ^operator.name initialize
               ^io.output-link <out>)
-->
    (<s> ^name takeoff)
    (<s> ^flight-mode vertical)
    (<out> ^target-altitude 5000)
    (<out> ^target-elevation 5000)
    (<out> ^throttle 0.0)
    (<s> ^landing no)
    (<out> ^autoflaps off)
    (<out> ^air-brake 0.0)
    (write (crlf) |Agent initialized.|)
